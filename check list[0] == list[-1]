test_data  = []

n = int(input("Enter the no of data in the given list: "))

for i in range(n):
    e = int(input())
    test_data.append(e)
    
print()
print("The given test_data: ",test_data)

def check(x):
    if test_data[0] == test_data[-1]:
        return True
    else:
        return False
    
s = check(test_data)

print(s)
